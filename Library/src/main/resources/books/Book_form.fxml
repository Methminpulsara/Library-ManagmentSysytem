<AnchorPane fx:id="mainancherpane" prefHeight="550.0" prefWidth="650.0" style="-fx-background-color: #2E073F;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.icet.ecom.controller.book.Book_form">
   <children>
      <AnchorPane layoutX="-1.0" prefHeight="550.0" prefWidth="283.0" style="-fx-background-color: #B1F0F7;">
         <children>
            <JFXTextField fx:id="Tisbn" focusColor="#6a1cd4" layoutX="40.0" layoutY="140.0" prefHeight="25.0" prefWidth="183.0" promptText="ISBN" unFocusColor="WHITE" />
            <JFXTextField fx:id="Ttitel" focusColor="#6a1cd4" layoutX="40.0" layoutY="178.0" prefHeight="25.0" prefWidth="183.0" promptText="titel" unFocusColor="WHITE" />
            <JFXTextField fx:id="Tauthor" focusColor="#6a1cd4" layoutX="40.0" layoutY="215.0" prefHeight="25.0" prefWidth="183.0" promptText="genre" text="author" unFocusColor="WHITE" />
            <JFXTextField fx:id="Tqty" focusColor="#6a1cd4" layoutX="40.0" layoutY="262.0" prefHeight="25.0" prefWidth="183.0" promptText="avelability" unFocusColor="WHITE" />
            <Label layoutX="31.0" layoutY="29.0" prefHeight="49.0" prefWidth="135.0" text="Books">
               <font>
                  <Font name="Bauhaus 93" size="33.0" />
               </font>
            </Label>
            <JFXButton fx:id="btnbackOnAction" layoutX="-1.0" layoutY="1.0" onAction="#btnbackOnAction" text="back">
               <graphic>
                  <ImageView fitHeight="17.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/back.png" />
                     </image>
                  </ImageView>
               </graphic>
            </JFXButton>
            <JFXTextField fx:id="Tbookid" focusColor="#6a1cd4" layoutX="42.0" layoutY="114.0" prefHeight="25.0" prefWidth="183.0" promptText="bookid" unFocusColor="WHITE" />
            <JFXButton layoutX="42.0" layoutY="380.0" onAction="#btnbookmanegmnetOnAction" prefHeight="25.0" prefWidth="183.0" style="-fx-background-color: #441752;" text="Book Manegment " textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </JFXButton>
            <JFXButton layoutX="40.0" layoutY="336.0" onAction="#btnAddOnAction" prefHeight="25.0" prefWidth="183.0" style="-fx-background-color: #69247C;" text="Add " textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </JFXButton>
         </children>
      </AnchorPane>
      <ImageView fitHeight="543.0" fitWidth="372.0" layoutX="233.0" layoutY="4.0" pickOnBounds="true" smooth="false" style="-fx-blend-mode: #2E073F;">
         <image>
            <Image url="@../images/Library%20Management%20System.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
